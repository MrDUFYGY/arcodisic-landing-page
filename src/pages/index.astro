---
import Layout from '../layouts/Layout.astro';

import Header from '../components/ui/indexui/Header.astro';
import Galeria from '../components/ui/indexui/Galeria.astro';
import Accesos from '../components/ui/indexui/Accesos.astro';
import Contacto from '../components/ui/indexui/ContactoSeccion.astro';
import Footer from '../components/ui/indexui/FooterPrincipal.astro';
import Map from '../components/ui/indexui/Map.astro';
import Procesos from '../components/ui/indexui/ProcesosDos.astro';
import Separador from '../components/ui/indexui/separadores/Separador.astro';

const metadata = {
  title: 'Home',
  ignoreTitleTemplate: true,
};
---

<Layout metadata={metadata}>
	
	<main>

		<Header />
		<Separador separador="Accesos" />
		<Accesos />
		<Separador separador="Galeria" />
		<Galeria />
		<Separador separador="Procesos" />
		<Procesos />
		<Separador separador="Contacto" />
		<Contacto />
	</main>
		<Separador separador="Mapa" />
		<Map />
		<Footer />
		  
</Layout>
